exercicio 6
#include <stdio.h>

int main()
{
    int num[4][4];
    int i,j,x,y;
    for(i=0; i<4; i++){
        for(j=0; j<4; j++){
            num[i][j] = 1 + rand() % 21;
            printf("%i ",num[i][j]);
            if(j==3)
                printf("\n");
        }
    }
   for(x=0; x<3; x++){
    if(x==0)
        y=1;
    if(x==1)
        y=2;
    if(x==2)
        y=3;
        for(;y<4;y++){
                num[x][y] = 0;
   }
   }
 for(i=0; i<4; i++){
        for(j=0; j<4; j++){
            printf("%i ",num[i][j]);
            if(j==3)
                printf("\n");
        }
 }
    return 0;
}
exercicio 7
#include <stdio.h>

int main()
{
    int num[5][5];
    int i,j,x,y;
    srand( (unsigned)time(NULL) );
    for(i=0; i<5; i++){
        for(j=0; j<5; j++){
            num[i][j] = rand() % 100;
            for(x=0; x<i; x++){
                  for(y=0; y<=j; y++){
                    if(num[i][j]==num[x][y]){
                    num[x][j] = rand() % 100;
                    } } }
                    printf("%i ",num[i][j]);
                     if(j==4)
                      printf("\n");
                      } }
    return 0;
}
