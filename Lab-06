exercicio 8
#include <stdio.h>

int main()
{
    int num[10];
    int i,j,k;
    for(i=0; i<10; i++){
        printf("digite um numero:");
        scanf("%i",&num[i]);
        for(j=0; j<i; j++){
        while(num[i]==num[j]){
        printf("digite um numero:");
        scanf("%i",&num[i]);
      }
     }
    }
    for(k=0; k<10; k++)
        printf("%i ",num[k]);

    return 0;
}
