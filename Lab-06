exercicio 4
#include <stdio.h>

int main()
{
    int num[5][5];
    int i,j,x,y=0;
    for(i=0; i<5; i++){
        for(j=0; j<5; j++){
            printf("digite um numero:");
            scanf("%i",&num[i][j]);
        }
    }
    printf("digite x:");
    scanf("%i",&x);
    for(i=0; i<5; i++){
        for(j=0; j<5; j++){
        if(x==num[i][j]){
        printf("x esta na linha %i e coluna %i\n",i+1,j+1);
        y=1;}
        }}
        if(y==0)
            printf("numero nao encontrado.");
    return 0;
}
