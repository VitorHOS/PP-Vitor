exercicio 7
#include <stdio.h>
#include <stdlib.h>
int main()
{
    int A[6],i,x,soma=0,resto,y=0;
    for(i=0; i<6; i++){
        printf("digite um numero:");
        scanf("%i",&A[i]);}
        for(x=0; x<6; x++){
        resto=A[x]%2;
   if(resto==0){
    printf("%i e par.\n",A[x]);
    soma +=A[x];
}
        }
   printf("a soma dos numeros pares e:%i\n",soma);
for(x=0; x<6; x++){
        resto=A[x]%2;
   if(resto!=0){
    printf("%i e impar.\n",A[x]);
    y++;
   }
}
printf("%i numeros impares digitados.",y);
    return 0;
}
