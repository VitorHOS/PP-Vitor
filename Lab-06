exercicio 8
#include <stdio.h>

int main()
{
    int num[10];
    int i,j,k;
    for(i=0; i<10; i++){
        printf("digite um numero:");
        scanf("%i",&num[i]);
        for(j=0; j<i; j++){
        while(num[i]==num[j]){
        printf("digite um numero:");
        scanf("%i",&num[i]);
      }
     }
    }
    for(k=0; k<10; k++)
        printf("%i ",num[k]);

    return 0;
}
MATRIZES
exercicio 1
#include <stdio.h>

int main()
{
    int num[4][4];
    int i,j,x;
    for(i=0; i<4; i++){
            for(j=0; j<4; j++){
        printf("digite um numero:");
        scanf("%i",&num[i][j]);
            if(num[i][j]>10)
                x++;
            }
    }
        printf("%i",x);

    return 0;
}
exercicio 2
#include <stdio.h>

int main()
{
    int num[5][5];
    int i,j,k,x,y;
    for(i=0; i<5; i++){
        num[0][i] = 1;
    }
    for(j=1; j<5; j++){
        for(k=0; k<5; k++)
            num[j][k] = 0;
    }
    for(x=0; x<5; x++){
        for(y=0; y<5; y++){
        printf("%i ",num[x][y]);
        if(y==4)
        printf("\n");
    }
    }
    return 0;
}
