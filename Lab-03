exercício 17
#include<stdio.h>

int main()
{
    float Bma,Bme,altura,area;
    printf("digite o valor da base maior:");
    scanf("%f",&Bma);
    printf("Digite o valor da base menor:");
    scanf("%f",&Bme);
    printf("digite a altura:");
    scanf("%f",&altura);
    if((Bma>0)&&(Bme>0)&&(altura>0)){
    area=(Bma+Bme)*altura/2;
    printf("a área do trapézio e %f",area);
    }else{
    printf("você digitou algum valor invalido");
    }
    return 0;
}
exercício 18
#include<stdio.h>

int main()
{
    float n1,n2,res;
    int opcao;
   
    printf("1-Adicao.\n");
    printf("2-Subtracao\n");
    printf("3-Multiplicacao\n");
    printf("4-Divisao\n");
    printf("escreva o número correspondente a opção desejada:");
    scanf("%i",&opcao);
    if(opcao==1){
    printf("Digite o primeiro número:");
    scanf("%f",&n1);
    printf("Digite o segundo numero:");
    scanf("%f",&n2);
    res=n1+n2;
    printf("%f",res); }
    if(opcao==2){
    printf("Digite o primeiro número:");
    scanf("%f",&n1);
    printf("Digite o segundo numero:");
    scanf("%f",&n2);
    res=n1-n2;
    printf("%f",res); }
    if(opcao==3){
    printf("Digite o primeiro número:");
    scanf("%f",&n1);
    printf("Digite o segundo numero:");
    scanf("%f",&n2);
    res=n1*n2;
    printf("%f",res); }
    if(opcao==4){
    printf("Digite o primeiro número:");
    scanf("%f",&n1);
    printf("Digite o segundo numero:");
    scanf("%f",&n2);
    res=n1/n2;
    printf("%f",res); }
    else {
    printf("opcao invalida"); }
    
    return 0;
}
exercício 19
#include<stdio.h>

int main()
{
    int x;
    printf("digite um número inteiro:");
    scanf("%i",&x);
    if(x%3==0)
    printf("%i e divisível por 3\n",x);
    if(x%5==0)
    printf("%i e divisível por 5",x);
    else 
    printf("%i não é divisível por 3 ou 5",x); 
    return 0;
}
exercício 20
#include<stdio.h>

int main()
{
    float a,b,c;
    printf("Digite um valor para a:");
    scanf("%f",&a);
    printf("Digite um valor para b:");
    scanf("%f",&b);
    printf("Digite um valor para c:");
    scanf("%f",&c);
    if((a+b>c)&&(a+c>b)&&(b+c>a)){
    if((a==b)&&(b==c))
    printf("é um triângulo equilatero");
    if((a==b)&&(a!=c)||(a==c)&&(a!=b)||(b==c)&&(b!=a))
    printf("o triangulo e isoceles");
    if((a!=b)&&(b!=c))
    printf("o trianguloe escaleno");
    }else{
    printf("nao pode ser um triângulo"); }
    return 0;
}
exercício 21
#include<stdio.h>

int main()
{
    int opcao;
    float n1,n2,res;
    printf("Escolha a opção:\n1- Soma de 2 números.\n2- Diferença entre 2 números (maior pelo menor).\n3- Produto entre 2 números.\n4- Divisão entre 2 números (o denominador não pode ser zero).\n");
    printf("digite o numero da opcao:");
    scanf("%i",&opcao);
    if(opcao==1){
    printf("Digite o primeiro numero:");
    scanf("%f",&n1);
    printf("Digite o segundo numero:");
    scanf("%f",&n2);
    res=n1+n2;
    printf("%f",res);}
    if(opcao==2){
    printf("Digite o primeiro numero:");
    scanf("%f",&n1);
    printf("Digite o segundo numero:");
    scanf("%f",&n2);
    if(n1>n2){
    res=n1-n2;
    printf("%f",res);
    }else{
    res=n2-n1;
    printf("%f",res); } }
    if(opcao==3){
    printf("Digite o primeiro numero:");
    scanf("%f",&n1);
    printf("Digite o segundo numero:");
    scanf("%f",&n2);
    res=n1*n2;
    printf("%f",res);}
    if(opcao==4){
    printf("Digite o numerador:");
    scanf("%f",&n1);
    printf("Digite o denominador:");
    scanf("%f",&n2);
    if(n2==0)
    printf("denominador invalido");
    else{
    res=n1/n2;
    printf("%f",res);}}
    
    return 0;
}

exercício 22
#include<stdio.h>

int main()
{
    int idade,tempo;
    printf("Digite sua idade:");
    scanf("%i",&idade);
    printf("Digite seus anos trabalhados:");
    scanf("%i",&tempo);
    if((idade>=60)&&(tempo>=25))
    printf("você pode se aposentar.");
    if((idade>=65)&&(tempo<25))
    printf("você pode se aposentar.");
    if((tempo>=30)&&(idade<65))
    printf("você pode se aposentar.");
    if((tempo<25)||(idade<65))
    printf("você não pode se aposentar.");
    return 0;
}
