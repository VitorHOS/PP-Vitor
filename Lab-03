exercicio 1
#include <stdio.h>
#include <stdlib.h>

int main()
{
    int n1,n2;
    printf("digite dois numero:");
    scanf("%i %i",&n1,&n2);
   if (n1>n2){
    printf("%i e maior que %i",n1,n2);
   }else {
   printf("%i e maior que %i",n2,n1);
   }
    return 0;
}
exercicio 2
#include <stdio.h>
#include <stdlib.h>

int main()
{
    int n,r;
    printf("digite um numero:");
    scanf("%i",&n);
    if (n>0){
    r= sqrt(n);
    printf("a raiz quadrada de %i e %i",n,r);
    }else{
    printf("numero invalido");
    }

    return 0;
}
exercicio 3
#include <stdio.h>
#include <stdlib.h>

int main()
{
    float n,r,q;
    printf("digite um numero:");
    scanf("%f",&n);
    if (n>0){
        r= sqrt(n);;
        printf("a raiz quadrada de %f e %f",n,r);
    }else {
    q=n*n;
    printf("o quadrado de %f e %f",n,q);
    }


    return 0;
}
exercicio 4
#include <stdio.h>
#include <stdlib.h>

int main()
{
    float n,r,q;
    printf("digite um numero:");
    scanf("%f",&n);
    if (n>0){
        r= sqrt(n);
        q=n*n;
        printf("a raiz quadrada de %f e %f\n",n,r);
        printf("o quadrado de %f e %f",n,q);
    }
    return 0;
}
