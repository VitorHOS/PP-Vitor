exercicio 1
#include <stdio.h>
#include <stdlib.h>

int main()
{
    int n1,n2;
    printf("digite dois numero:");
    scanf("%i %i",&n1,&n2);
   if (n1>n2){
    printf("%i e maior que %i",n1,n2);
   }else {
   printf("%i e maior que %i",n2,n1);
   }
    return 0;
}
exercicio 2
#include <stdio.h>
#include <stdlib.h>

int main()
{
    int n,r;
    printf("digite um numero:");
    scanf("%i",&n);
    if (n>0){
    r= sqrt(n);
    printf("a raiz quadrada de %i e %i",n,r);
    }else{
    printf("numero invalido");
    }

    return 0;
}
exercicio 3
#include <stdio.h>
#include <stdlib.h>

int main()
{
    float n,r,q;
    printf("digite um numero:");
    scanf("%f",&n);
    if (n>0){
        r= sqrt(n);;
        printf("a raiz quadrada de %f e %f",n,r);
    }else {
    q=n*n;
    printf("o quadrado de %f e %f",n,q);
    }


    return 0;
}
exercicio 4
#include <stdio.h>
#include <stdlib.h>

int main()
{
    float n,r,q;
    printf("digite um numero:");
    scanf("%f",&n);
    if (n>0){
        r= sqrt(n);
        q=n*n;
        printf("a raiz quadrada de %f e %f\n",n,r);
        printf("o quadrado de %f e %f",n,q);
    }
    return 0;
}
exercicio 5
#include <stdio.h>
#include <stdlib.h>

int main()
{
    int n,r;
    printf("digite um numero:");
    scanf("%i",&n);
    r=n%2;
    if (r==0){
    printf("o numero e par");
    }else {
    printf("o numero e impar");
    }


    return 0;
}
exercicio 6
#include <stdio.h>
#include <stdlib.h>

int main()
{
    int n1,n2,d;
    printf("digite dois numeros:");
    scanf("%i %i",&n1,&n2);
    if (n1>n2){
        d=n1-n2;
        printf("%i e maior que %i e a diferenca entre eles e %i",n1,n2,d);
    }else {
    d=n2-n1;
    printf("%i e maior que %i e a diferenca entre eles e %i",n2,n1,d);
    }
    return 0;
}
exercicio 7
#include <stdio.h>
#include <stdlib.h>

int main()
{
    int n1,n2;
    printf("digite dois numeros:");
    scanf("%i %i",&n1,&n2);
    if (n1>n2){
        printf("o maior que %i",n1);
    }
    if (n2>n1) {
    printf("o maior que e %i",n2);
    }
    if (n1==n2){
        printf("os numeros sao iguais");
    }
    return 0;
}
exercicio 8
#include <stdio.h>
#include <stdlib.h>

int main()
{
    float nota1,nota2,media;
    printf("digite duas notas:");
    scanf("%f %f",&nota1,&nota2);
    if (nota1>=0.0 && nota1<=10.0 && nota2>=0.0 && nota2<=10.0){
    media=(nota1+nota2)/2;
    printf("a media das notas e %f",media);
    } else {
    printf("as notas sao invalidas.");
    }


    return 0;
}

exercicio 9
#include <stdio.h>
#include <stdlib.h>

int main()
{
    float salario,emprestimo,p;
    printf("digite o salario:");
    scanf("%f",&salario);
    printf("digite o valor da prestacao do emprestimo:");
    scanf("%f",&emprestimo);
    p=salario/5;
    if (emprestimo>p){
       printf("emprestimo nao concedido");
    } else {
    printf("emprestimo comcedido");
    }
    return 0;
}

exercicio 10
#include<stdio.h>

int main()
{
    float altura,pesoI;
    char sexo;
    printf("Digite sua altura em metros:");
    scanf("%f",&altura);
    printf("digete seu sexo em M ou F:");
    scanf(" %c",&sexo);
    if(sexo=='M') {
        pesoI=altura*72.7-58;
        printf("seu peso ideal e %f",pesoI);
    }
    if(sexo=='F') {
        pesoI=altura*62.1-44.7;
        printf("peso ideal feminino %f",pesoI);
    }
    return 0;
}
exercício 11
#include<stdio.h>

int main()
{
    
    int n,soma=0;
    printf("Digite um numero positivo maior que 0:");
    scanf("%i",&n);
    if(n<=0){
    printf("numero invalido");
    }else{
    while(n>0){
    soma=soma+(n%10);
    n/=10;
    }
    printf("a soma dos algarismos e %i",soma);
    }
    
    return 0;
}

exercício 12
#include<stdio.h>
#include<math.h>
int main()
{
    int x,l;
    printf("Digite um numero:");
    scanf("%i",&x);
    if (x>0) {
    l=log10(x);
    printf("o logaritmo de %i na base 10 e %i",x,l);
    } else {
    printf("numero invalido");
    }


    return 0;
}

exercício 13
#include<stdio.h>

int main()
{
   float n1,n2,n3,nt;
    printf("Digite a primeira nota:");
    scanf("%f",&n1);
    printf("Digite a segunda nota:");
    scanf("%f",&n2);
    printf("Digite a terceira nota(peso2):");
    scanf("%f",&n3);
    nt=(n1+n2+n3+n3)/4;
    if(nt>=60){
    printf("você passou com %f",nt);
    } else {
    printf("voce não passou.");
    }
    return 0;
}
exercício 14
#include<stdio.h>

int main()
{
   float n1,n2,n3,media;
    printf("Digite a nota do trabalho:");
    scanf("%f",&n1);
    printf("Digite a nota da avaliação semestral:");
    scanf("%f",&n2);
    printf("Digite a nota do exame final:");
    scanf("%f",&n3);
    media=(n1*2+n2*3+n3*5)/10;
    if(media<3)
    printf("você reprovou.");
    if((media>=3)&&(media<5))
    printf("voce está de recuperação.");
    if(media>=5)
    printf("você passou.");
    
    return 0;
}
exercício 15
#include<stdio.h>

int main()
{
    int n;
    printf("digite um numero de 1 a 7:");
    scanf("%i",&n);
    switch (n) {
    case 1: printf("domingo");break;
    case 2: printf("segunda");break;
    case 3: printf("terça");break;
    case 4: printf("quarta");break;
    case 5: printf("quinta");break;
    case 6: printf("sexta");break;
    case 7: printf("sábado");break; 
    }
    return 0;
}
exercício 16
#include<stdio.h>

int main()
{
    int n;
    printf("digite um numero de 1 a 12:");
    scanf("%i",&n);
    switch (n) {
    case 1: printf("Janeiro");break;
    case 2: printf("Fevereiro");break;
    case 3: printf("Março");break;
    case 4: printf("Abril");break;
    case 5: printf("Maio");break;
    case 6: printf("Junho");break;
    case 7: printf("Julho");break; 
    case 8: printf("Agosto");break;
    case 9: printf("Setembro");break;
    case 10: printf("Outubro");break;
    case 11: printf("Novembro");break;
    case 12: printf("Dezembro");break;  
     }
    return 0;
}
exercício 17
#include<stdio.h>

int main()
{
    float Bma,Bme,altura,area;
    printf("digite o valor da base maior:");
    scanf("%f",&Bma);
    printf("Digite o valor da base menor:");
    scanf("%f",&Bme);
    printf("digite a altura:");
    scanf("%f",&altura);
    if((Bma>0)&&(Bme>0)&&(altura>0)){
    area=(Bma+Bme)*altura/2;
    printf("a área do trapézio e %f",area);
    }else{
    printf("você digitou algum valor invalido");
    }
    return 0;
}
exercício 18
#include<stdio.h>

int main()
{
    float n1,n2,res;
    int opcao;
   
    printf("1-Adicao.\n");
    printf("2-Subtracao\n");
    printf("3-Multiplicacao\n");
    printf("4-Divisao\n");
    printf("escreva o número correspondente a opção desejada:");
    scanf("%i",&opcao);
    if(opcao==1){
    printf("Digite o primeiro número:");
    scanf("%f",&n1);
    printf("Digite o segundo numero:");
    scanf("%f",&n2);
    res=n1+n2;
    printf("%f",res); }
    if(opcao==2){
    printf("Digite o primeiro número:");
    scanf("%f",&n1);
    printf("Digite o segundo numero:");
    scanf("%f",&n2);
    res=n1-n2;
    printf("%f",res); }
    if(opcao==3){
    printf("Digite o primeiro número:");
    scanf("%f",&n1);
    printf("Digite o segundo numero:");
    scanf("%f",&n2);
    res=n1*n2;
    printf("%f",res); }
    if(opcao==4){
    printf("Digite o primeiro número:");
    scanf("%f",&n1);
    printf("Digite o segundo numero:");
    scanf("%f",&n2);
    res=n1/n2;
    printf("%f",res); }
    else {
    printf("opcao invalida"); }
    
    return 0;
}
exercício 19
#include<stdio.h>

int main()
{
    int x;
    printf("digite um número inteiro:");
    scanf("%i",&x);
    if(x%3==0)
    printf("%i e divisível por 3\n",x);
    if(x%5==0)
    printf("%i e divisível por 5",x);
    else 
    printf("%i não é divisível por 3 ou 5",x); 
    return 0;
}
exercício 20
#include<stdio.h>

int main()
{
    float a,b,c;
    printf("Digite um valor para a:");
    scanf("%f",&a);
    printf("Digite um valor para b:");
    scanf("%f",&b);
    printf("Digite um valor para c:");
    scanf("%f",&c);
    if((a+b>c)&&(a+c>b)&&(b+c>a)){
    if((a==b)&&(b==c))
    printf("é um triângulo equilatero");
    if((a==b)&&(a!=c)||(a==c)&&(a!=b)||(b==c)&&(b!=a))
    printf("o triangulo e isoceles");
    if((a!=b)&&(b!=c))
    printf("o trianguloe escaleno");
    }else{
    printf("nao pode ser um triângulo"); }
    return 0;
}
