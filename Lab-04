    printf("%i\n",x);
    return 0;
}
exercicio 12
#include <stdio.h>

int main()
{
    int n,soma=0,x,y=1,re;
    printf("digite um numero:");
    scanf("%i",&n);
    for(x=0; x<n-1; x++){
        re=n%y;
        if(re==0)
        soma+=y;
        y++;
    }
  printf("%i",soma);

    return 0;
}
exercicio13
#include <stdio.h>

int main()
{
    int n,soma=0,x,y=1000,re3,re5;

    for(x=0; x<=1000; x++){
        re3=y%3;
        re5=y%5;
        if((re3==0)||(re5==0))
        soma+=re3;
        y--;

    }
  printf("%i",soma);

    return 0;
}

exercicio14
#include <stdio.h>
#include <stdlib.h>

int main()
{
    int n,d1,d2,x;
    printf("digite a quantidade de dados:");
    scanf("%i",&n);
    for(x=0; x<n; x++){
     d1= rand () % 6;
     d2= rand () % 6;
     printf("dado 1: %i\n",d1);
     printf("dado 2: %i\n",d2);
     if(d1==d2)
      printf("sao iguais.\n");
     if(d1>d2)
     printf("o primeiro dado e maior.\n");
     if(d2>d1)
     printf("o segundo dado dado e maior.\n");

}
    return 0;
}
exercicio 15
#include <stdio.h>
#include <stdlib.h>

int main()
{
    int x,n,qua,cubo,raiz;

    do{
     printf("digite um numero:");
     scanf("%i",&n);
    if(n>0){
    qua=n*n;
    cubo=n*n*n;
    raiz=sqrt(n);
    printf("o quadrado de %i e %i.\n",n,qua);
    printf("o cubo de %i e %i.\n",n,cubo);
    printf("a raiz quadrada de %i e %i\n",n,raiz);
    }
    }while(n>0);{
}

     printf("fim");
    return 0;
}
