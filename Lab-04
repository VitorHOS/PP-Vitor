Exercício 1
#include<stdio.h> 

int main()
{
    int x=3;
    while(x<12){
    printf("%i\n",x);
    x=x+3;}
    return 0;
}
Exercício 2
#include<stdio.h> 

int main()
{
    int x,y=1,z=1;
    printf("FOR:");
    for(x=1;x<=100;x++)
    printf("%i ",x);
    printf("\nWHILE:");
    while(y<=100){
    printf("%i ",y);
    y++;}
    printf("\nDO-WHILE:");
    do{
    printf("%i ",z);
    z++;
    }while(z<=100);
    return 0;
}
Exercício 3
#include<stdio.h> 

int main()
{
    int x=10;
    while(x>=0) {
        printf("%i\n",x);
        x--;
    }
   printf("Fim");
    return 0;
}
