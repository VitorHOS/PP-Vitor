exercicio 17
#include <stdio.h>
#include <stdlib.h>

int main()
{
    float c=1.5,z=1.1;
    int a;
    while(c>z){
        c+=0.02;
        z+=0.03;
        a++;
    }
    printf("ira demorar %i anos.\n",a);
    return 0;
}
exercicio 18
TESTE TESTE TESTE TESTE
#include <stdio.h>
#include <stdlib.h>

int main()
{
    float s=2000,au=1.5;
    int a,atual=2022;
    s*=au;
    for(a=1997; a<=atual; a++){
     s*=au;
     au*=2;
    }
    printf("em 2020 o salario sera &f",s);
    return 0;
}

