                res+=soma;
                x++;       
    }
    media=res/10;
        printf("a soma dos 10 numeros são %i",media);
        return 0;
}
Exercício 7
#include<stdio.h> 

int main()
{
        int x=0,n1=0,res=0,media;
        while(x<10) {
        printf("Digite um numero:");
        scanf("%i",&n1);
        if(n1>0) {
            res+=n1;
            x++; } }
        media=res/10;
        printf("a soma dos 10 numeros são %i",media);
        return 0;
}
