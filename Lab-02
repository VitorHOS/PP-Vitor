    printf("o numero e: %f",n1);
    return 0;

Exercício 3
#include<stdio.h> 

int main()
{
   int s,n1,n2,n3;
     printf("Digite um numero:");
    scanf("%i",&n1);
    printf("Digite outro numero:");
    scanf("%i",&n2);
    printf("Digete outro numero:");
    scanf("%i",&n3);
    s=n1+n2+n3;
    printf("a soma dos numeros e:%i",s);
    return 0;
} 

Exercício 4
#include<stdio.h> 

int main()
{
    int n1,qn1;
    scanf("%i",&n1);
    qn1=n1*n1;
    printf("o quadrado de %i e:%i",n1,qn1);
    return 0;
} 

Exercício 5
#include<stdio.h> 

int main()
{
    float n1,x;
    scanf("%f",&n1);
    x = n1 / 5;
    printf("a quinta parte do número e:%f",x);
    return 0
} 

Exercício 6
#include<stdio.h> 

int main()
{
    float F,C;
    printf("digite uma temperatura em celcius:");
    scanf("%f",&C);
    F = C*(9.0/5.0)+32.0;
    printf("%f graus celcius em Fahrenheit e %f",C,F);
    return 0;
} 

Exercício 7
#include<stdio.h> 

int main()
{
    float F,C;
    printf("digite uma temperatura em Fahrenheit:");
    scanf("%f",&F);
    C = 5.0*(F- 32.0)/9.0;
    printf("%f graus Fahrenheit em celcius e %f",F,C);
    return 0;
} 

Exercício 8
#include<stdio.h> 

int main()
{
    float K,C;
    printf("digite uma temperatura em Kelvin:");
    scanf("%f",&K);
    C = K-273.15;
    printf("%f graus Kelvin em celcius e %f",K,C);
    return 0; 

Exercício 9
#include<stdio.h> 

int main()
{
    float K,C;
    printf("digite uma temperatura em Celcius:");
    scanf("%f",&C);
    K = C + 273.15;
    printf("%f graus Celcius em Kelvin e %f",C,K);
    return 0;
} 

Exercício 10
#include<stdio.h> 

int main()
{
    float K,M;
    printf("digete uma velocidade em Km/h:");
    scanf("%f",&K);
     M = K/3.6;
    printf("%f Km/h é %f m/s",K,M);
    return 0;
}

