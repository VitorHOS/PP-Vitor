Exercício 46
#include<stdio.h> 

int main()
{
    int x,y,z,n1,nc;
    printf("Digite um numero inteiro de 3 digitos:");
    scanf("%i",&n1);
    x=n1/100;
    y=(n1%100)/10;
    z=n1%10;
    nc=z*100+y*10+x;
    printf("o contrário de %i é %i",n1,nc);
    return 0;
} 

Exercício 47
#include<stdio.h> 

int main()
{
    int w,x,y,z,n1;
    printf("Digite um numero inteiro de 4 digitos:");
    scanf("%i",&n1);
    w=n1/1000;
    x=(n1%1000)/100;
    y=(n1%1000%100)/10;
    z=n1%1000%100%10;
    printf("%i\n%i\n%i\n%i",w,x,y,z);
    return 0;
} 

Exercício 48
#include<stdio.h> 

int main()
{
    int st,s,m,h;
    printf("Digite um tempo em segundos:");
    scanf("%i",&st);
    m=st/60;
    s=st%60;
    h=m/60;
    m=m%60;
    printf("%i segundos corresponde a %i horas %i minutos e %i segundos.",st,h,m,s);
    return 0;
} 

Exercício 49
#include<stdio.h> 

int main()
{
    int Si,Mi,Hi,Sf,Mf,Hf,d,h,m,s;
    printf("Digite o horário do começo do experimento:\n");
    printf("hora:");
    scanf("%i",&Hi);
    printf("minuto:");
    scanf("%i",&Mi);
    printf("segundos:");
    scanf("%i",&Si);
    printf("Digite a duração do experimento em segundos");
    scanf("%i",&d);
    m=d/60;
    s=d%60;
    h=m/60;
    m=m%60;
    Hf=Hi+h;
    Mf=Mi+m;
    Sf=Si+s;
    printf("o experimento acabou as %i horas %i minutos e %i segundos",Hf,Mf,Sf);
    
    return 0;
} 

