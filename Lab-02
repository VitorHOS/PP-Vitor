Exercício 42
#include<stdio.h> 

int main()
{
    float sb,sf,x,y;
    printf("digite seu salário base:");
    scanf("%f",&sb);
    x=sb*1.05;
    y=sb*0.07;
    sf=x-y;
    printf("seu salário com descontos e bônus ficará %f",sf);
    return 0;
} 

Exercício 43
#include<stdio.h> 

int main()
{
    float vp,vd,v3x,cd,c3x;
    printf("digite o valor:");
    scanf("%f",&vp);
    vd=vp*0.9;
    v3x=vp/3;
    cd=vd*0.05;
    c3x=vp*0.05;
    printf("o valor com desconto será %f\n",vd);
    printf("o valor da parcelas em 3x será %f\n",v3x);
    printf("sua comissão na venda com desconto será %f\n",cd);
    printf("sua comissão na venda em 3x será %f\n",c3x);
    
    return 0;
} 

Exercício 44
#include<stdio.h> 

int main()
{
    int altura,alturad,degraus;
    printf("qual altura dos degraus em centimetros:");
    scanf("%i",&alturad);
    printf("qual altura do local a ser subido em centimetros:");
    scanf("%i",&altura);
    degraus=altura/alturad;
    printf("sera necessário %i degraus",degraus);
    return 0;
} 

