Exercício 39
#include<stdio.h> 

int main()
{
    float g1,g2,g3,x=780000;
    g1=x*0.46;
    g2=x*0.32;
    g3=x*0.22;
    printf("o primeiro ganhará %f \no segundo ganhará %f\no terceiro ganhará %f",g1,g2,g3);
    return 0;
} 

Exercício 40
#include<stdio.h> 

int main()
{
    float dias,pagamento,x,ir=0.92;
    printf("Digite quantos dias trabalhou:");
    scanf("%f",&dias);
    x=dias*30;
    pagamento=x*ir;
    printf("você receberá %f.",pagamento);
    return 0;
}
