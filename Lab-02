    return 0;
} 

Exercício 51 


Exercício 52
#include<stdio.h> 

int main()
{
    float vi1,vi2,vi3,vti,g1,g2,g3,vp,x;
    printf("Digite quanto cada um investiu\n");
    printf("investidor1:");
    scanf("%f",&vi1);
    printf("investidor2:");
    scanf("%f",&vi2);
    printf("investidor3:");
    scanf("%f",&vi3);
    printf("digite o valor do premio:");
    scanf("%f",&vp);
    vti=vi1+vi2+vi3;
    x=vp/vti;
    g1=x*vi1;
    g2=x*vi2;
    g3=x*vi3;
    printf("o ganho do primeiro investidor é %f\ndo segundo e %f\ndo terceiro e %f ",g1,g2,g3);
    
    return 0;
} 

Exercício 53
#include<stdio.h> 

int main()
{
    float c,l,p,d;
    printf("Digite o comprimento do terreno em metros:");
    scanf("%f",&c);
    printf("digite a largura do terreno em metros:");
    scanf("%f",&l);
    printf("digite o preço do metro da tela:");
    scanf("%f",&p);
    d=c*l;
    p=p*d;
    printf("o preço da tela em um terreno d dimensão %f e %f",d,p);
    return 0;
}
