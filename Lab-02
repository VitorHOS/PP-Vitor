    printf("o numero e: %f",n1);
    return 0;

Exercício 3
#include<stdio.h> 

int main()
{
   int s,n1,n2,n3;
     printf("Digite um numero:");
    scanf("%i",&n1);
    printf("Digite outro numero:");
    scanf("%i",&n2);
    printf("Digete outro numero:");
    scanf("%i",&n3);
    s=n1+n2+n3;
    printf("a soma dos numeros e:%i",s);
    return 0;
} 

Exercício 4
#include<stdio.h> 

int main()
{
    int n1,qn1;
    scanf("%i",&n1);
    qn1=n1*n1;
    printf("o quadrado de %i e:%i",n1,qn1);
    return 0;
} 

Exercício 5
#include<stdio.h> 

int main()
{
    float n1,x;
    scanf("%f",&n1);
    x = n1 / 5;
    printf("a quinta parte do número e:%f",x);
    return 0
} 

Exercício 6
#include<stdio.h> 

int main()
{
    float F,C;
    printf("digite uma temperatura em celcius:");
    scanf("%f",&C);
    F = C*(9.0/5.0)+32.0;
    printf("%f graus celcius em Fahrenheit e %f",C,F);
    return 0;
} 

Exercício 7
#include<stdio.h> 

int main()
{
    float F,C;
    printf("digite uma temperatura em Fahrenheit:");
    scanf("%f",&F);
    C = 5.0*(F- 32.0)/9.0;
    printf("%f graus Fahrenheit em celcius e %f",F,C);
    return 0;
} 

Exercício 8
#include<stdio.h> 

int main()
{
    float K,C;
    printf("digite uma temperatura em Kelvin:");
    scanf("%f",&K);
    C = K-273.15;
    printf("%f graus Kelvin em celcius e %f",K,C);
    return 0; 

Exercício 9
#include<stdio.h> 

int main()
{
    float K,C;
    printf("digite uma temperatura em Celcius:");
    scanf("%f",&C);
    K = C + 273.15;
    printf("%f graus Celcius em Kelvin e %f",C,K);
    return 0;
} 

Exercício 10
#include<stdio.h> 

int main()
{
    float K,M;
    printf("digete uma velocidade em Km/h:");
    scanf("%f",&K);
     M = K/3.6;
    printf("%f Km/h é %f m/s",K,M);
    return 0;
}

Exercício 11
#include<stdio.h> 

int main()
{
    float K,M;
    printf("digete uma velocidade em m/s:");
    scanf("%f",&M);
    K = M*3.6;
    printf("%f m/s e %f km/h",M,K);
    return 0;
} 

Exercício 12
#include<stdio.h> 

int main()
{
    float K,M;
    printf("digete uma distância em milhas:");
    scanf("%f",&M);
    K = 1.61*M;
    printf("%f milhas corresponde a %f quilômetros",M,K);
    return 0;
} 

Exercício 13
#include<stdio.h> 

int main()
{
    float K,M;
    printf("digete uma distância em quilômetros:");
    scanf("%f",&K);
    M = K/1.61;
    printf("%f quilômetros corresponde a %f milhas",K,M);
    return 0;
} 

Exercício 14
#include<stdio.h> 

int main()
{
    const float pi=3.141592;
    float G,R;
    printf("digite um angulo em graus:");
    scanf("%f",&G);
    R = G*pi/180;
    printf("%f graus em radianos e %f",G,R);
    return 0;
} 

Exercício 15
#include<stdio.h> 

int main()
{
    const float pi=3.141592;
    float G,R;
    printf("digite um angulo em radianos:");
    scanf("%f",&R);
    G = R*180/pi;
    printf("%f radianos em graus e %f",R,G);
    return 0;
} 

Exercício 16
#include<stdio.h> 

int main()
{
    float P,C;
    printf("digite um comprimento em polegadas:");
    scanf("%f",&P);
    C = P*2.54;
    printf("%f polegadas sao %f centímetros",P,C);
    return 0;
} 

Exercício 17
#include<stdio.h> 

int main()
{
    float P,C;
    printf("digite um comprimento em centimetros:");
    scanf("%f",&C);
    P = C/2.54;
    printf("%f centimetros sao %f polegadas",C,P);
    return 0;
} 

Exercício 18
#include<stdio.h> 

int main()
{
    float L,M;
    printf("digite um volume em metros cubicos:");
    scanf("%f",&M);
    L = 1000*M;
    printf("%f metros cúbicos são %f litros",M,L);
    return 0;
} 

Exercício 19
#include<stdio.h> 

int main()
{
    float L,M;
    printf("digite um volume em litros:");
    scanf("%f",&L);
    M = L /1000;
    printf("%f litros sao %f metros cúbicos",L,M);
    return 0;
}
