Exercício 28
#include<stdio.h>
#include<stdlib.h> 

int main()
{
    int n1,n2,n3,sq;
    printf("digite um numeros:");
    scanf("%i",&n1);
    printf("digite outro numeros:");
    scanf("%i",&n2);
    printf("digite outro numeros:");
    scanf("%i",&n3);
    n1*=n1;
    n2*=n2;
    n3*=n3;
    sq=n1+n2+n3;
    printf("a soma dos quadrados dos 3 números e %i",sq);
    return 0;
} 

Exercício 29
#include<stdio.h> 

int main()
{
    float nota1,nota2,nota3,nota4,media;
    printf("digite a primeira nota:");
    scanf("%f",&nota1);
    printf("digite a segunda nota:");
    scanf("%f",&nota2);
    printf("digite a terceiranota:");
    scanf("%f",&nota3);
    printf("digite a quarta nota:");
    scanf("%f",&nota4);
    media=(nota1+nota2+nota3+nota4)/4;
    printf("a media das notas e %f",media);
    return 0;
} 

Exercício 30
#include<stdio.h> 

int main()
{
    float real,dolar,cota;
    printf("digite o valor em real para conversão:");
    scanf("%f",&real);
    printf("digite a cotação do dólar hoje:");
    scanf("%f",&cota);
    dolar=real/cota;
    printf("%f reais em dólares é %f dólares",real,dolar);
    return 0;
} 

Exercício 31
#include<stdio.h> 

int main()
{
    int x,suc,ant;
    printf("digite um numero inteiro");
    scanf("%i",&x);
    ant=x-1;
    suc=x+1;
    printf(" o antecessor e %i \n o sucessor e %i",ant,suc);
    return 0;
}

Exercício 32
#include<stdio.h> 

int main()
{
    int x,st,ad,s;
    printf("digite um numero inteiro");
    scanf("%i",&x);
    st=x*3+1;
    ad=x*2-1;
    s=st+ad;
    printf("a soma do sucessor do seu triplo com o antecessor do seu dobro e %i",s);
    return 0;
} 

