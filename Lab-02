Exercício 34
#include<stdio.h> 

int main()
{
    const float pi=3.141592;
    float raio,area;
    printf("Digite o raio do circulo:");
    scanf("%f",&raio);
    area=pi*raio;
    printf("a area de um círculo de raio = %f e %f",raio,area);
    return 0;
} 

Exercício 35
#include<stdio.h>
#include<math.h> 

int main()
{
    float a,b,hipotenusa;
    printf("digite um valor para a:");
    scanf("%f",&a);
    printf("digite um valor para b:");
    scanf("%f",&b);
    hipotenusa= sqrt(a*a+b*b);
    printf("a hipotenusa é %f",hipotenusa);
    
    return 0;
} 

Exercício 36
#include<stdio.h> 

int main()
{
    const float pi=3.141592;
    float raio,altura,volume;
    printf("Digite o raio do cilindro:");
    scanf("%f",&raio);
    printf("digite a altura do cilindro ");
    scanf("%f",&altura);
    volume = pi * raio * altura;
    printf("o volume é %f",volume);
    return 0;
} 

